<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.social.socialnetwork.Controller.LogInController">
   <children>
      <VBox layoutX="18.0" layoutY="25.0" prefHeight="283.0" prefWidth="564.0">
         <children>
            <GridPane prefHeight="200.0" prefWidth="309.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="99.0" minHeight="10.0" prefHeight="76.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="108.60000000000001" minHeight="10.0" prefHeight="108.60000000000001" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="116.40000000000003" minHeight="10.0" prefHeight="98.40000000000003" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label prefHeight="18.0" prefWidth="204.0" text="LOG IN BELOW" />
                  <Label prefHeight="18.0" prefWidth="110.0" text="username" GridPane.rowIndex="1" />
                  <Label prefHeight="18.0" prefWidth="118.0" text="password " GridPane.rowIndex="2" />
                  <PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="usernameField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <ButtonBar prefHeight="36.0" prefWidth="564.0">
              <buttons>
                <Button fx:id="logInButton" mnemonicParsing="false" onAction="#OnLogInAction" text="log in" />
              </buttons>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
